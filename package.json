{
  "name": "photo-app-hybrid",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",

    "start": "node server.js",
    "server": "node server.js",

    "dev:frontend": "npm --prefix frontend run dev",
    "dev:backend": "node server.js",

    "dev:all": "start \"backend\" cmd /c \"node server.js\" && start \"frontend\" cmd /c \"npm --prefix frontend run dev\"",

    "scan-faces": "node scan-faces.js",
    "scan-faces:test": "node scan-faces.js --limit 10 --verbose",
    "scan-duplicates": "node scan-duplicates.js",
    "scan-duplicates:test": "node scan-duplicates.js --limit 100 --dry-run --verbose",
    "populate-paths": "node populate-photo-paths.js",
    "populate-paths:test": "node populate-photo-paths.js --limit 10 --verbose --dry-run"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "@tensorflow/tfjs-backend-wasm": "^4.22.0",
    "@vladmandic/face-api": "^1.7.15",
    "canvas": "^2.11.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "express-validator": "^7.3.1",
    "firebase-admin": "^12.0.0",
    "imghash": "^1.1.2",
    "onnxruntime-node": "^1.17.0",
    "sharp": "^0.34.5",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.23",
    "gdown": "^1.0.6",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.1.0"
  }
}
